{
  "docSuffix": ".md",
  "docTagStart": "#*c:",
  "docTagEnd": "*",
  "srcSuffix": ".coffee",
  "srcTagStart": "#*c:",
  "srcTagEnd": "*",
  "outputSuffix": ".litcoffee",
  "defaultTagOrder": ["untagged", "description"],
  "dirs": [
    {
      "src": "src/lib",
      "docs": "src/docs",
      "output": "out/docs/src"
    },
    {
      "src": "src/test",
      "docs": "src/test_docs",
      "output": "out/docs/test"
    }
  ]
}
